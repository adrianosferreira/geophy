{
  "name": "geophy/geo-gateway",
  "description": "Property Gateway API",
  "license": "MIT",
  "authors": [
    {
      "name": "Adriano Ferreira",
      "email": "adrianokta@gmail.com"
    }
  ],
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "firebase/php-jwt": "^5.0",
    "slim/slim": "^3.1",
    "tuupola/slim-jwt-auth": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": ">=5.0",
    "roave/security-advisories": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "start": "php -S localhost:8888 -t public",
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {"App\\": "src/"}
  }
}
